  "dependencies": {
    "async": "",
    "basic-auth": "^1.0.0",
    "body-parser": "^1.9.0",
    "cookie-session": "",
    "debug": "",
    "express": "4",
    "formage-mongoose-types": "",
    "lodash-contrib": "",
    "lru-cache": "",
    "mpromise": "^0.5.5",
    "node-ckeditor": "",
    "nodestrum": "",
    "serve-static": "^1.6.3",
    "mongoose": "^4.2.4",
    "kerberos": "~0.0"
  },
  "description": "Admin GUI addon for mongoose, jugglingdb, or just as a form generator",
  "devDependencies": {
    "bower": "",
    "chai": "",
    "grist": "latest",
    "istanbul": "",
    "jade": "0.35.x",
    "jshint": "",
    "jugglingdb": "",
    "mocha": ""
  },
  "directories": {
    "example": "./example"
  },
  "engines": {
    "node": "> 0.10.0"
  },
  "jshintConfig": {
    "esnext": true,
    "expr": true,
    "forin": true,
    "freeze": true,
    "globals": {
      "Promise": false,
      "after": true,
      "before": true,
      "describe": true,
      "expect": true,
      "it": true
    },
    "immed": true,
    "maxdepth": 4,
    "newcap": true,
    "noarg": true,
    "node": true,
    "noempty": true,
    "nonew": true,
    "noyield": true,
    "scripturl": true,
    "sub": true,
    "supernew": true,
    "trailing": true,
    "undef": true,
    "unused": true
  },
  "keywords": [
    "admin",
    "forms",
    "cross-database",
    "MongoDB",
    "MongoDB-admin",
    "MongoDB-GUI",
    "grist",
    "mongoose",
    "mongoose-gui",
    "juggling",
    "jugglingdb",
    "backoffice",
    "cms"
  ],
  "licenses": [
    {
      "type": "MIT",
      "url": "https://raw.github.com/Empeeric/formage/master/LICENSE"
    }
  ],
  "main": "lib/Formage.js",
  "name": "formage",
  "optionalDependencies": {
    "asynctrace": ""
  },
  "publishConfig": {
    "tag": "beta"
  },
  "repository": "Empeeric/formage",
  "scripts": {
    "cover": "istanbul cover ./node_modules/mocha/bin/_mocha -x generated/**",
    "generate": "node utils/CompileTempletes.js",
    "lint": "jshint .",
    "mypublish": "npm run setassets && npm version patch --version-with-shrinkwrap && git push --follow-tags",
    "prepublish": "npm run generate",
    "setassets": "npm run generate && bower i && git clean -X -d -f assets",
    "start": "node example/classic/app",
    "test": "npm run generate && mocha"
  },
  "version": "3.2.27"
}
